<table class = "table table-striped"
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
    <% @enter.each do |e| %>
    <tr>
      <td><%= e.name %></td>
      <td><%= e.email %></td>
    </tr>
    <% end %>
  </tbody>
</table>
    


<%= will_paginate @enter %>
