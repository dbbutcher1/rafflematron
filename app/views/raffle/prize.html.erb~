<div class = "well span6 offset3 form-horizontal">
  <div class = "control-group">
    <div class = "control-label">
      <h1><%= @raffle.name %></h1>
      <p>Unique id: <%= @raffle.id %></p>
      <p>Prizes: <%= @raffle.num_prizes %></p> 
      <p>Entries: <%= @raffle.num_entries %></p>
    </div>
  </div>
<%= form_for @raffle, :method => :post, :url => raffle_completed_path, :html => { :multipart => true } do |f| %>
  <%= f.hidden_field :id %>
  <%= render :partial => "prize", :collection => @prizes %>
  <div class = "control-group">
     <%= f.submit "Submit", :class => "control" %>
   </div>
<% end %>
</div>
